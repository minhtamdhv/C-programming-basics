//Viết chương trình nhập giá trị cho hai biến nguyên i, j và in ra kết quả của các biểu thức: i > j, i < j, i >= j, i <= j, i == j, i != j.

#include <stdio.h>
int main(){
  int i, j;
	printf("\nNhap gia tri i,j : ");
	scanf("%d%d", &i, &j);
	//i > j;
	printf(" cho kết quả i>j %d\n", i > j);
	//i < j;
	printf("cho kết quả i<j %d\n", i < j);
	//i >= j;
	printf("cho kết quả	i >= j %d\n", i >= j);
	//i <= j;
	printf("cho kết quả	i <= j %d\n", i <= j);
	//i == j;
	printf("cho kết quả i == j %d\n", i == j);
	//i != j;
	printf("cho kết quả i != j %d\n", i != j);
	return 0;
}
