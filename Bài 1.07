//Nhập vào một số thực x, tính giá trị của hàm số f(x) = x^3 – 2x^2 + 5x +1.            
#include <stdio.h>
int main() {
   float x;
   int a=1;
   int b=2;
   int c=5;
   int d=1;
   printf("nhâp số thực x: ");
   scanf("%f",&x);
   float fx =  a*x*x*x + b*x*x + c*x + d;
   printf("\ngiá trị của hàm số là: %.2f",fx);
   return 0;
}
