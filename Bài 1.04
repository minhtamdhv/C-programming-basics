//Viết chương trình nhập 3 số nguyên a, b và c. In ra màn hình tổng  a^2 + b^2 + c^2.
 #include <stdio.h>
    int main()
    {	
       int a,b,c;
       printf("nhap a,b và c là: " 	);
       scanf("%d%d%d", &a,&b,&c);
       int kq = a*a + b*b + c*c;
       printf("gia tri cua bieu thuc la: %d", kq);
       return 0;
    }
